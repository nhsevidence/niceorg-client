    "@nice-digital/nds-jquery": "^1.2.1",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.2.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
